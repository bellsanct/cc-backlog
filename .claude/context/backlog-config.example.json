{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "spaceKey": "my-space",
  "apiEndpoint": "https://my-space.backlog.com/api/v2",
  "defaultProject": null,
  "preferences": {
    "defaultIssueType": "Task",
    "defaultPriority": "Normal",
    "autoAssignSelf": true,
    "notifyOnCreate": true,
    "cacheTimeout": 300,
    "batchSize": 10,
    "rateLimitDelay": 100
  },
  "api": {
    "timeout": 30000,
    "retryAttempts": 3,
    "retryDelay": 1000
  }
}
